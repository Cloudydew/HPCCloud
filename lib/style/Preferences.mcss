.linkSection {
    display: flex;
    justify-content: center;
}

.link {
    display: inline-block;
    padding: 60px 30px;
    width: 140px;
    text-align: center;
    border: 1px solid lightgrey;
    margin: 5px;
    transition: background-color 2s linear;
}

.link:hover{
    background-color: lightblue;
}
