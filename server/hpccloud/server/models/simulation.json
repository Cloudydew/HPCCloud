{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "id":"#simulation",
    "required": ["name", "steps"],
    "additionalProperties": false,
    "properties": {
        "name": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "active": {
            "type": "string"
        },
        "disabled": {
            "type": "array",
            "item": {
                "type": "string"
            }
        },
        "status": {
            "enum": ["editing", "running", "data"]
        },
        "steps": {
            "type": "object",
            "patternProperties": {
                "^.*$": {
                    "$ref": "#/step"
                }
            }
        },
        "projectId": {

        },
        "folderId": {

        },
        "access": {

        },
        "userId": {

        },
        "_id": {

        },
        "created": {

        },
        "updated": {

        }
    }
}
